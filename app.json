{
  "expo": {
    "name": "petcare",
    "slug": "petcare",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sauterdev.petcare"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.sauterdev.petcare"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "773ff077-1e2f-42fa-8910-13eb552656c9"
      }
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Permita que $(PRODUCT_NAME) acceda a sus fotos",
          "cameraPermission": "Permita que $(PRODUCT_NAME) acceda a su c√°mara"
        }
      ]
    ],
    "scheme": "petcare",
    "owner": "sauterdev"
  }
}
